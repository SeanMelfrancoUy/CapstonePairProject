#Bivariate Analysis

#This Boxplot shows the distribution between the categorical variable explicit and the discrete variable danceability.
ggplot(data = SpotifyTracks, mapping = aes(x = explicit, y = danceability)) +
  geom_boxplot()

#This Scatterplot shows the covariation between the continuous variable duration_ms and the discrete variable energy. 
ggplot(data = SpotifyTracks) +
  geom_point(mapping = aes(x = duration_ms, y = energy))

#This 2d Histogram shows the distribution between the two discrete variables key and tempo.
ggplot(data = SpotifyTracks) +
  geom_bin2d(mapping = aes(x = key, y = tempo))

  #install.packages("hexbin")
  ggplot(data = smaller) +
    geom_hex(mapping = aes(x = key, y = tempo))
